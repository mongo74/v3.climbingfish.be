      <SimplePage id="1305" parentID="1295" level="3" creatorID="0" sortOrder="0" createDate="2015-08-17T22:45:36" updateDate="2015-10-16T03:14:40" nodeName="Praktijkverhalen" urlName="praktijkverhalen" path="-1,1083,1295,1305" isDoc="" nodeType="1288" creatorName="Benoit" writerName="Benoit" writerID="0" template="1151" nodeTypeAlias="SimplePage">
