      <SimplePage id="1303" parentID="1090" level="3" creatorID="0" sortOrder="3" createDate="2015-05-04T14:50:46" updateDate="2015-10-09T11:10:27" nodeName="Bedrijven" urlName="bedrijven" path="-1,1083,1090,1303" isDoc="" nodeType="1288" creatorName="Benoit" writerName="Benoit" writerID="0" template="1151" nodeTypeAlias="SimplePage">
