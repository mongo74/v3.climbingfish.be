      <SimplePage id="1333" parentID="1292" level="3" creatorID="0" sortOrder="2" createDate="2015-10-02T16:05:19" updateDate="2015-10-16T03:14:39" nodeName="Mindmappen" urlName="mindmappen" path="-1,1083,1292,1333" isDoc="" nodeType="1288" creatorName="Benoit" writerName="Benoit" writerID="0" template="1151" nodeTypeAlias="SimplePage">
