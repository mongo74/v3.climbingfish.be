      <SimplePage id="1348" parentID="1091" level="3" creatorID="0" sortOrder="3" createDate="2015-10-05T16:27:47" updateDate="2015-10-16T03:14:39" nodeName="Bibliografie" urlName="bibliografie" path="-1,1083,1091,1348" isDoc="" nodeType="1288" creatorName="Benoit" writerName="Benoit" writerID="0" template="1151" nodeTypeAlias="SimplePage">
