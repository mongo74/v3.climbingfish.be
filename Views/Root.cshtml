@inherits UmbracoTemplatePage
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="Everybody is a Genius: with the right tools - Iedereen is een Genie: met de juiste middelen | @CurrentPage.metaDescription" />
    <meta name="keywords" content="Leren leren, studiebegeleiding, mindmapping, leerprocessen, brainstorming technieken, particulieren, bedrijven, studenten" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <!-- Facebook tags -->
    <meta property="og:title" content="@CurrentPage.displayName | Studiebegeleiding en Mindmapping | The Climbing Fish" />
    <meta property="og:url" content="http://www.climbingfish.be/" />
    <meta property="og:description" content="Everybody is a Genius: with the right tools - Iedereen is een Genie: met de juiste middelen | @CurrentPage.metaDescription" />
    <meta property="og:image" content="http://www.climbingfish.be/media/1021/logo150px.jpg" />
    <meta property="article:author" content="https://www.facebook.com/marina.deroover" />
    <title>@CurrentPage.displayName | Studiebegeleiding en Mindmapping | The Climbing Fish</title>
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300italic,300,400italic,400,600italic,600,700italic,700,800italic,800" rel="stylesheet" type="text/css">
    <link href="~/Content/climbingfish.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/justifiedGallery.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/modernizr-2.6.2.js"></script>
    @RenderSection("headerScripts", required: false)
</head>
<body>
    <div id="fb-root"></div>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/nl_NL/sdk.js#xfbml=1&version=v2.0";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>
    @Html.Partial("_ClimbingFishheader")

    @RenderBody()

    @Umbraco.RenderMacro("Doormat", new { Doormat = "1110" })
    @RenderSection("scripts", required: false)

    <!-- Go to www.addthis.com/dashboard to customize your tools -->
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-580ce0a0c2f4f2cd"></script>


</body>
</html>
<script type="text/javascript" src="~/Scripts/jquery.justifiedGallery.min.js"></script>
@RenderSection("Bottomscripts", required: false)